{% extends 'default/base.html.twig' %}

{% block content %}

  {% for message in app.flashes('Exito') %}
      <div class="flash-notice">
          {{ message }}
      </div>
  {% endfor %}

  {% for url in urls %}
    <a href="{{url.url}}" >{{ url.title }}</a><br/>
  {% endfor %}

  {{ form_start(form) }}
  {{ form_widget(form,{ 'attr': {'class': 'form-control'} }) }}
  {{ form_end(form) }}

{% endblock content %}
